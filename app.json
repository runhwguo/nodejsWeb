{
  "name": "schoolResourceShareApp",
  "script": "start.js",
  "cwd": "./",
  "instances": 0,
  "watch": [
    "src",
    "static",
    "view",
    "dist"
  ],
  "ignoreWatch": [
    "node_modules",
    "build",
    "log"
  ],
  "max_memory_restart": "400M",
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "exec_mode": "cluster",
  "watch_options": {
    "followSymlinks": false
  },
  "error_file": "log/app_err.log",
  "out_file": "log/app_out.log",
  "env": {
    "NODE_ENV": "production"
  },
  "env_test": {
    "NODE_ENV": "test"
  }
}